{
    "name": "bookflow/core",
    "description": "A multi-tenant booking system SaaS",
    "type": "project",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "BookFlow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BookFlow\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.4",
        "firebase/php-jwt": "^7.0",
        "google/apiclient": "^2.19",
        "symfony/mailer": "*",
        "vlucas/phpdotenv": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.40"
    },
    "config": {
        "sort-packages": true,
        "process-timeout": 2000
    }
}
